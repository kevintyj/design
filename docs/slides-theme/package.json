{
	"name": "@kevintyj/design/docs/slides-theme",
	"version": "0.0.0",
	"type": "module",
	"keywords": [
		"slidev-theme",
		"slidev"
	],
	"engines": {
		"node": ">=18.0.0"
	},
	"scripts": {
		"build": "slidev build example.md",
		"dev": "slidev example.md --open --port 3080",
		"export": "slidev export example.md",
		"screenshot": "slidev export example.md --format png"
	},
	"dependencies": {
		"@slidev/types": "^52.0.0"
	},
	"devDependencies": {
		"@slidev/cli": "^52.0.0"
	},
	"//": "Learn more: https://sli.dev/guide/write-theme.html",
	"slidev": {
		"colorSchema": "both",
		"defaults": {
			"fonts": {
				"sans": "Inter",
				"mono": "Cygnus Mono",
				"serif": "Recia Variable"
			}
		}
	}
}
